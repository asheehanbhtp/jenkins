{
  "name": "jenkins",
  "description": "A sample application for working on the CI/CD process within Jenkins",
  "version": "0.0.0",
  "main": "index.js",
  "devDependencies": {
    "@types/chai": "^4.0.10",
    "@types/mocha": "^2.2.44",
    "@types/node": "^8.5.1",
    "chai": "^4.1.2",
    "dotenv": "^4.0.0",
    "mocha": "^4.0.1",
    "nodemon": "^1.13.3",
    "nsp": "^3.1.0",
    "nyc": "^11.3.0",
    "rimraf": "^2.6.2",
    "ts-node": "^4.0.2",
    "tslint": "^5.8.0",
    "tslint-microsoft-contrib": "^5.0.1",
    "typemoq": "^2.1.0",
    "typescript": "^2.6.2",
    "@types/body-parser": "^1.16.8",
    "@types/express": "^4.0.39"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "reflect-metadata": "^0.1.10",
    "typedi": "^0.6.0",
    "body-parser": "^1.18.2",
    "express": "^4.16.2",
    "routing-controllers": "^0.7.6"
  },
  "scripts": {
    "prestart": "nsp check",
    "start": "ts-node --cache --cache-directory=.ts-node --inspect=0.0.0.0:9229 ./src/index.ts",
    "start:watch": "nodemon -e ts --ignore \"src/**/*.spec.ts\" --exec npm start",
    "test": "nyc mocha -r ts-node/register \"src/**/*.spec.ts\"",
    "test:watch": "npm test -- --watch --watch-extensions ts",
    "heroku-postbuild": "npm run build",
    "prebuild": "npm run clean && npm run lint && npm test",
    "build": "tsc",
    "clean": "rimraf .nyc_output coverage dist",
    "lint": "tslint --project tsconfig.json --type-check \"src/**/*.ts\"",
    "serve": "node dist/index.js"
  },
  "engines": {
    "node": "7.9.0"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [],
